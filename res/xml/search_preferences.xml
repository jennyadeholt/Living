<?xml version="1.0" encoding="utf-8"?>
<PreferenceScreen xmlns:android="http://schemas.android.com/apk/res/android" >

    <PreferenceCategory
            android:title="Area">

        <EditTextPreference
                android:key="preferences_area_location"
                android:title="Search area"
                android:defaultValue="MalmÃ¶"
                android:inputType="text" />
    </PreferenceCategory>

    <PreferenceCategory
            android:title="Number of rooms"
            android:summary="">
        <ListPreference
                android:key="preference_min_numbers"
                android:title="Min number of rooms"
                android:entries="@array/rooms"
                android:defaultValue="3"
                android:entryValues="@array/rooms" />
        <ListPreference
                android:key="preference_max_numbers"
                android:title="Max number of rooms"
                android:entries="@array/rooms"
                android:defaultValue="4"
                android:entryValues="@array/rooms" />

    </PreferenceCategory>

    <PreferenceCategory
            android:title="@string/preferences_type_of_building_title">

        <MultiSelectListPreference
                android:key="preference_building_type"
                android:title="@string/preferences_type_of_building"
                android:entries="@array/building"
                android:defaultValue="@array/building_types"
                android:entryValues="@array/building_types" />
    </PreferenceCategory>

</PreferenceScreen>